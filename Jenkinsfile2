pipeline {
    agent any 
        tools{
            maven 'M2_HOME'
        }
        stages{
            stage('sonarqube scan'){
                steps{
                    withSonarQubeEnv('sonarQube'){  
                    sh 'mvn verify org.sonarsource.scanner.maven:sonar-maven-plugin:sonar -Dsonar.projectKey=sewa2023_week16project'
                }
                }
            }
            stage('all maven commands'){
                steps{
                    sh 'mvn clean test compile install package'
                }
            }
            stage('Upload artifact'){
                steps{
                    sh 'curl -uadmin:AP7tZ35XC1QdhnhhjBvUhzamyDS -T target/bio*.jar "http://3.85.1.184:8081/artifactory/geoapp/"'
                }
            }
   
        }
    }
